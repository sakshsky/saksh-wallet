{
    "name": "sakshsky/saksh-wallet",
    "description": "A simple wallet system for Laravel applications",
    "type": "library",
    "version": "1.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "susheel kumar",
            "email": "susheel@sakshsky.com"
        }
    ],
    "keywords": ["laravel", "wallet", "transactions", "balance", "payment"],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^8.2",
        "illuminate/support": "^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Saksh\\Wallet\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Saksh\\Wallet\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Saksh\\Wallet\\SakshWalletServiceProvider"
            ]
        }
    }
}